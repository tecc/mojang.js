{
  "name": "mojang.js",
  "version": "0.1.0-alpha",
  "description": "mojang.js is a simple wrapper for Mojangs APIs, written in TypeScript.",
  "main": "dist/index.js",
  "types": "dist/lib.d.ts",
  "directories": {
    "example": "./examples",
    "lib": "./dist"
  },
  "scripts": {
    "prepublish": "npm test",
    "install": "node scripts/symlinkDirs.js",
    "pretest": "npm run build",
    "test": "node scripts/test.js",
    "prebuild": "rm -rf dist/",
    "build": "tsc --build tsconfig.json",
    "postbuild": "npm run types && git add dist/",
    "types": "node scripts/genTypes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tecc/mojang.js.git"
  },
  "keywords": [
    "mojang",
    "minecraft"
  ],
  "author": "tecc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tecc/mojang.js/issues"
  },
  "homepage": "https://github.com/tecc/mojang.js#readme",
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@types/pkg-dir": "^2.0.1",
    "@types/superagent": "^4.1.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "pkg-dir": "^5.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "superagent": "^6.1.0",
    "uuid": "^8.3.2"
  }
}
